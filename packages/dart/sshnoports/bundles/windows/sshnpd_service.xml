<service>
  <id>sshnpd</id>
  <name>sshnpd</name>
  <description>Ssh No Ports Daemon</description>
  <depend>Netlogon<depend/>
  <serviceaccount>
    <username>NT AUTHORITY\NetworkService</username>
  </serviceaccount>
  <executable>sshnpd</executable>
  <arguments>-a {client} -m {manager} -d {device} -k {homdir} -s</arguments>
</service>